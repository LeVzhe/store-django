    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Products tests",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "test",
                "products.tests"
            ],
            "django": true
        },
        {
            "name": "RUNSERVER DJANGO",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "runserver"
            ],
            "django": true,
            "autoStartBrowser": false,
            "justMyCode": true,
            "program": "${workspaceFolder}\\manage.py"
        }
    ]
}